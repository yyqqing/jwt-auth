{
    "name": "yyqqing/jwt-auth",
    "description": "JSON Web Token Authentication for Lumen forked from lsxiao/jwt-auth",
    "authors": [
        {
            "name": "yyqqing",
            "email": "y_yqing@qq.com"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "illuminate/auth": "~5.1",
        "illuminate/contracts": "~5.1",
        "illuminate/http": "~5.1",
        "illuminate/support": "~5.1"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Lsxiao\\JWT\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "keywords": [
        "jwt",
        "lumen",
        "laravel",
        "laravel5",
        "json web token"
    ],
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "prefer-stable": true
}